//I caught these Pokemon in the forrest today.
//I will be describing the Pokemon with Pokemon types. In the code to be logged into the console.

var pokemon = ['Raichu', 'Squirtle', 'Jigglypuff']
var pokemonTypes = ['Electric', 'Water', 'Normal', 'Fairy']

